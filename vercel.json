{
  "version": 2,
  "builds": [
    {
      "src": "chat-app-frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "chat-app-frontend/dist/index.html"
    }
  ],
  "buildCommand": "cd chat-app-frontend && npm install && npm run build",
  "outputDirectory": "chat-app-frontend/dist"
}
