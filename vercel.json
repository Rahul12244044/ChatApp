{
  "version": 2,
  "builds": [
    {
      "src": "chat-app-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "chat-app-frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "chat-app-frontend/dist/index.html"
    }
  ]
}
